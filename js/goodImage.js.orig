    function toObject(ob) {
        var o = {}, ln = ob.length, i = 0, a = [];

        for (i; i < ln; i++) {
            o[i] = ob[i];
        }
        i = 0;
        for (i in o) {
            if (o[i] instanceof Array) {
                a[i] = o[i];
                o[i] = {};
                $.extend(o[i], a[i]);
            }
        }
        return o;
    }
    ;

    function toArray(ob) {
        var o = [], i, ln = 0, a = {}, k;

        for (i in ob) {
            o[i] = [];
            o[i] = ob[i];
        }
        
        ln = o.length; 
        i = 0;
        
        for (i; i < ln; i++) {
            if (o[i] instanceof Object) {
                a = o[i];
                o[i] = [];
                for (k in a) {
                    o[i][k] = a[k];
                }
            }
        }
        return o;
    }
